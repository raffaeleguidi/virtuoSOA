# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

# Home page
GET     /route                           	controllers.Administration.route()
GET     /route/:id                          controllers.Administration.editRoute(id: String)
POST    /route                     			controllers.Administration.addRoute()
POST    /saveroute                     		controllers.Administration.saveRoute()
GET     /routes                    			controllers.Administration.getRoutes()

GET     /							      	controllers.Application.proxyGet(path = "")
POST    /                                 	controllers.Application.proxyPost(path = "")
GET     /*path                           	controllers.Application.proxyGet(path)
POST    /*path                           	controllers.Application.proxyPost(path)

